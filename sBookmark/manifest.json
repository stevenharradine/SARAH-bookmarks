{
  "manifest_version": 2,

  "name": "sBookmark",
  "description": "Load your bookmarks from SARAH",
  "version": "1.0",

  "permissions": [
	"tabs",
	"bookmarks",
    "http://192.168.1.2/",
	"http://*/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}